/* package/choosePartner/index.wxss */
page{
  background-color: #f5f5f5;
}
.i-index-demo, page {
  width: 100%;
  height: 100%;
}

.i-index-demo-item {
  padding: 10px;
  border-bottom: #ddd solid 1rpx;
  font-size: 14px;
  overflow: hidden;
}
.i-index-demo-item .radio{
  float: left;
  overflow: hidden;
  line-height: 112rpx;
}

.i-index-demo-item .more {
  width: 40rpx;
  height: 40rpx;
  float: right;
  margin-right: 33rpx;
  margin-top: 39rpx;
}
.i-index-demo-item .headIcon{
  width: 99rpx;
  height: 99rpx;
  border-radius: 50%;
  background: red;
  float: left;
  margin-left: 23rpx;
  margin-right: 33rpx;
}
