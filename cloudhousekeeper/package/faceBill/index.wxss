/* package/faceBill/index.wxss */
page{
  background-color: #f5f5f5;
}
.boxTop{
  position: absolute;
  top: 0;
  bottom: 100rpx;
  width: 100%;
}
.add{
  position: absolute;
  bottom: 0;
  line-height: 100rpx;
  text-align: center;
  background-color: rgb(46,160,255);
  color: #ffff;
  width: 100%;
  font-size: 33rpx;
}
.banner{
  width: 100%;
  height: 280rpx;
}
.noBill{
  width: 100%;
  text-align: center;
  padding: 40rpx 0;
  color: red;
}
.warning{
  width: 44rpx;
  height: 44rpx;
  position: relative;
  top: 5rpx;
  margin: 0 10rpx;
}
.menu{
  overflow: hidden;
  background: #fff;
  padding: 40rpx 20rpx;
  box-sizing: border-box;
}
.tip{
  font-size: 33rpx;
  line-height: 40rpx;
}
.tips{
  margin-top: 55rpx;
  font-size: 27rpx;
}
.menu button{
  line-height: 90rpx;
  background-color: green;
  color: #fff;
  font-size: 33rpx;
  margin: 31rpx 0;
}
.list .cell-index--i-cell-ft{
  color:rgb(46,160,255);
}
.boxTop .overBill{
  background: #f5f5f5;
}
.default{
  background:rgb(46,160,255);
  border-radius:20rpx;
  color:#fff;
  top:29rpx;
  position:absolute;
  right:76rpx;
  padding:5rpx 20rpx;
  font-size:25rpx;
}