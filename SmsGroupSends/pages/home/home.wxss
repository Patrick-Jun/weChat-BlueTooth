.header_bg{
  width: 100%;
  height: 482rpx;
  background: #fff;
  /* vertical-align: middle; */
  background-size: cover;
}
.content {
  position: absolute;
  top: 30rpx;
  z-index: 10;
}
.inputContent {
  display: flex;
  display: -webkit-flex;
  width: 690rpx;
  height: 88rpx;
  margin: 0  30rpx;
  line-height: 88rpx;
  border-radius: 10rpx;
  background: #fff;
}

.inputContent input {
  flex-grow: 2;
  height: 88rpx;
  line-height: 88rpx;
  padding-left: 34rpx;
  font-size: 28rpx;
}

.voice {
  width: 36rpx;
  height: 45rpx;
  margin: 20rpx 40rpx 0 40rpx;
}

.scan {
  width: 40rpx;
  height: 40rpx;
  margin-right: 30rpx;
  margin-top: 25rpx;
  vertical-align: middle;
}

.headNav {
  display: flex;
  display: -webkit-flex;
  margin-top: 40rpx;
}

.navItem {
  flex: 1;
  border-right: 1px solid #2d7ae5;
  text-align: center;
}

.count {
  font-size: 65rpx;
  color: #fff;
}

.textFont {
  font-size: 28rpx;
  color: #fff;
}
.middleContainer {
  position: absolute;
  width: 100%;
  margin-top: -93rpx;
  background-color: #fff;
}
.middle-top ,.middleBottom{
  display: flex;
  display: -webkit-flex;
  border-bottom: 1px solid #eee;
  /* padding-bottom: 34rpx; */
}
.middleBottom {

  border-bottom: none;
}
.topItem,.bottomItem {
  flex: 1;
  text-align: center;
 padding-bottom: 44rpx;
}
.itemImg {
  width: 128rpx;
  height: 128rpx;
  background: gold;
  border-radius: 50%;
  margin-top: 50rpx;
}
.topItem text ,.bottomItem text{
  font-size: 28rpx;
  color: #333;
}

